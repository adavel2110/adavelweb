		"@tailwindcss/vite": "^4.1.14",
		"@vitest/browser-playwright": "^4.0.5",
		"autoprefixer": "^10.4.22",
		"carbon-icons-svelte": "^13.6.0",
		"playwright": "^1.56.1",
		"postcss": "^8.5.6",
